@model Person

<div class="row">
    <div class="col-sm-2">
        <p>@Model.Name</p>
    </div>
    <div class="col-sm-2">
        <p>@Model.City</p>
    </div>
    <div class="col-sm-2">
        <p>@Model.PhoneNumber</p>
    </div>
    <div class="col-sm-2">
        <p>@Html.ActionLink("X", "DeletePerson", "Register", new { id = Model.Id }) </p>
    </div>
</div>